{
  "name": "@superfaceai/station",
  "version": "0.0.1",
  "description": "Superface Station utility",
  "main": "dist/index.js",
  "repository": "git@github.com:superfaceai/station.git",
  "author": "Superface Team",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.release.json --outDir dist",
    "clean": "rimraf dist/",
    "prebuild": "yarn clean",
    "compile": "node dist/compile.js",
    "create:profile": "node dist/create-profile.js",
    "create:provider": "node dist/create-provider.js",
    "create:map": "node dist/create-map.js",
    "test": "jest",
    "test:clean": "jest --clear-cache && jest",
    "lint": "eslint ./",
    "lint:fix": "yarn lint --fix",
    "format": "prettier -c ./",
    "format:fix": "prettier --write ./",
    "prepush": "yarn build && yarn test && yarn lint && yarn format"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.15",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.2",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@superfaceai/ast": "0.0.22",
    "@superfaceai/parser": "0.0.15",
    "@superfaceai/sdk": "0.0.14",
    "rimraf": "^3.0.2"
  }
}