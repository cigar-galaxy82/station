{
  "name": "@superfaceai/station",
  "version": "0.0.0",
  "description": "Superface Station utility",
  "main": "dist/src/index.js",
  "repository": "git@github.com:superfaceai/station.git",
  "author": "Superface Team",
  "bin": {
    "station": "bin/station",
    "st": "bin/station"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "test": "dotenv -e .env.capabilities -- jest",
    "test:compile": "superface compile && superface check && yarn test",
    "test:clean": "dotenv -e .env.capabilities -- jest --clear-cache && jest",
    "test:debug": "DEBUG=* yarn test",
    "lint": "eslint ./",
    "lint:fix": "yarn lint --fix",
    "format": "prettier -c ./",
    "format:fix": "prettier --write ./",
    "prepush": "yarn lint:fix && yarn format:fix",
    "publish": "node ./github/workflows/publish.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@superfaceai/cli": "^0.0.20",
    "dotenv-cli": "^4.0.0"
  }
}
