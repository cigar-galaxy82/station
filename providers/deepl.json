{
  "name": "deepl",
  "services": [
    {
      "id": "default",
      "baseUrl": "https://{SUBDOMAIN}.deepl.com"
    }
  ],
  "defaultService": "default",
  "securitySchemes": [
    {
      "id": "apikey",
      "type": "apiKey",
      "in": "query",
      "name": "auth_key"
    }
  ],
  "parameters": [
    {
      "name": "SUBDOMAIN",
      "default": "api-free",
      "description": "API subdomain; set to 'api' for DeepL API Pro, or 'api-free' for DeepL API Free (default)."
    }
  ]
}
