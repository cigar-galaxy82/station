{
  "name": "bitbucket",
  "services": [
    {
      "id": "default",
      "baseUrl": "https://api.bitbucket.org"
    }
  ],
  "securitySchemes": [
    {
      "id": "o_auth_bearer_token",
      "type": "http",
      "scheme": "bearer"
    },
    {
      "id": "o_auth_token_in_query",
      "type": "apiKey",
      "in": "query",
      "name": "access_token"
    },
    {
      "id": "o_auth_token_in_body",
      "type": "apiKey",
      "in": "body",
      "name": "access_token"
    },
    {
      "id": "basic",
      "type": "http",
      "scheme": "basic"
    }
  ],
  "defaultService": "default"
}
