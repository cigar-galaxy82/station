{
  "name": "vonage-nexmo",
  "services": [
    {
      "id": "default",
      "baseUrl": "https://rest.nexmo.com"
    }
  ],
  "defaultService": "default",
  "securitySchemes": [
    {
      "id": "apiKey",
      "type": "apiKey",
      "in": "body",
      "name": "api_key"
    }
  ],
  "parameters": [
    {
      "name": "apiSecret",
      "description": "Your API secret"
    }
  ]
}
