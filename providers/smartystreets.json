{
  "name": "smartystreets",
  "services": [
    {
      "id": "default",
      "baseUrl": "https://us-street.api.smartystreets.com"
    }
  ],
  "securitySchemes": [
    {
      "id": "auth-token",
      "type": "apiKey",
      "in": "query",
      "name": "auth-token"
    }
  ],
  "defaultService": "default",
  "parameters": [
    {
      "name": "AUTH_ID",
      "description": "Your Auth ID"
    }
  ]
}
