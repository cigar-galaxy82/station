{
  "name": "github",
  "services": [
    {
      "id": "default",
      "baseUrl": "https://api.github.com"
    }
  ],
  "securitySchemes": [
    {
      "id": "basic",
      "type": "http",
      "scheme": "basic"
    },
    {
      "id": "bearer_token",
      "type": "http",
      "scheme": "bearer"
    }
  ],
  "defaultService": "default"
}
