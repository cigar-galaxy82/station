{
  "profiles": {
    "communication/send-message": {
      "file": "./grid/communication/send-message/profile.supr",
      "providers": {
        "twilio": {
          "file": "./grid/communication/send-message/maps/twilio.suma",
          "defaults": {
            "SendMessage": {
              "input": {
                "from": "+13345090396",
                "TWILIO_SID": "$TWILIO_USERNAME"
              }
            }
          }
        },
        "tyntec": {
          "file": "./grid/communication/send-message/maps/tyntec.suma",
          "defaults": {
            "SendMessage": {
              "input": {
                "from": "tyntec"
              }
            }
          }
        }
      }
    },
    "vcs/user-repos": {
      "file": "./grid/vcs/user-repos/profile.supr",
      "providers": {
        "github": {
          "file": "./grid/vcs/user-repos/maps/github.suma"
        },
        "gitlab": {
          "file": "./grid/vcs/user-repos/maps/gitlab.suma"
        },
        "bitbucket": {
          "file": "./grid/vcs/user-repos/maps/bitbucket.suma"
        }
      }
    },
    "vcs/pull-requests": {
      "file": "./grid/vcs/pull-requests/profile.supr",
      "providers": {
        "gitlab": {
          "file": "./grid/vcs/pull-requests/maps/gitlab.suma"
        },
        "bitbucket": {
          "file": "./grid/vcs/pull-requests/maps/bitbucket.suma"
        },
        "github": {
          "file": "./grid/vcs/pull-requests/maps/github.suma"
        }
      }
    },
    "vcs/single-file-content": {
      "file": "./grid/vcs/single-file-content/profile.supr",
      "providers": {
        "github": {
          "file": "./grid/vcs/single-file-content/maps/github.suma"
        },
        "gitlab": {
          "file": "./grid/vcs/single-file-content/maps/gitlab.suma"
        },
        "bitbucket": {
          "file": "./grid/vcs/single-file-content/maps/bitbucket.suma"
        }
      }
    }
  },
  "providers": {
    "github": {
      "file": "../providers/github.json",
      "security": []
    },
    "twilio": {
      "file": "../providers/twilio.json",
      "security": [
        {
          "id": "basic",
          "username": "$TWILIO_USERNAME",
          "password": "$TWILIO_PASSWORD"
        }
      ]
    },
    "tyntec": {
      "file": "../providers/tyntec.json",
      "security": [
        {
          "id": "apikey",
          "apikey": "$TYNTEC_API_KEY"
        }
      ]
    },
    "gitlab": {
      "file": "../providers/gitlab.json",
      "security": []
    },
    "bitbucket": {
      "file": "../providers/bitbucket.json",
      "security": [
        {
          "id": "basic",
          "username": "$BITBUCKET_USERNAME",
          "password": "$BITBUCKET_PASSWORD"
        }
      ]
    }
  }
}
