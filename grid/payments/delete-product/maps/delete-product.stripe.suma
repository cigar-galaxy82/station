profile = "payments/delete-product@1.0"
provider = "stripe"

map deleteProduct {
  http DELETE "/products/{input.id}" {
    security "basic"
    request {

    }

    response 200 "application/json" {
      map result {
        result: "OK"
      }
    }

    response "application/json" {
      map error {
        title = body.error.message
      }
    }
  }
}
