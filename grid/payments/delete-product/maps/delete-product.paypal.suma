profile = "payments/delete-product@1.0"
provider = "paypal"

map deleteProduct {
  http DELETE "/catalogs/products/{input.id}" {
    security "basic"
    request {

    }

    response 200 "application/json" {
      map result {
          result: "OK"
      }
    }

    response "application/json" {
      map error {
        title = body.message
        detail = body.details
      }
    }
  }
}
