profile = "payments/delete-plan@1.0"
provider = "stripe"

"""
Delete Plan map
"""

map deletePlan {
  http DELETE "/plans/{input.id}" {
    security "basic"

    response 200 "application/json" {
      map result {
        result: "OK"
      }
    }

     response "application/json" {
         map error {
            title = body.error.message
         }
     }
  }
}
